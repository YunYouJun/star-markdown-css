{
  "name": "star-markdown-css",
  "type": "module",
  "version": "0.5.1",
  "packageManager": "pnpm@9.4.0",
  "description": "Markdown Css about starry.",
  "author": "YunYouJun <me@yunyoujun.cn>",
  "license": "MIT",
  "homepage": "https://smc.yunyoujun.cn",
  "repository": "https://github.com/YunYouJun/star-markdown-css.git",
  "keywords": [
    "browser",
    "markdown",
    "css",
    "scss",
    "style",
    "stylesheet",
    "star",
    "starry",
    "planet",
    "blood"
  ],
  "main": "dist/planet/planet-markdown.min.css",
  "files": [
    "dist/**/*.css",
    "src/**/*.scss"
  ],
  "scripts": {
