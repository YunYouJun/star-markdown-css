    "@types/node": "^20.14.10",
    "@types/sass": "^1.45.0",
    "@unocss/eslint-plugin": "^0.61.3",
    "@yunyoujun/logger": "^0.3.5",
    "bumpp": "^9.4.1",
    "clean-css": "^5.3.3",
    "consola": "^3.2.3",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "fs-extra": "^11.2.0",
    "gzip-size": "^7.0.0",
    "sass": "^1.77.6",
    "tsx": "^4.16.2",
